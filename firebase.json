{
  "hosting": {
    "public": "dist/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "ssr"
      }
    ],
    "headers": [{
      "source": "**/*.*",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "https://eoriolfront-staging.firebaseapp.com"
      }]
    }]
  },
  "functions": {
    "source": "dist"
  }
}
